SELECT sku,precio_unitario FROM productos WHERE precio_unitario > 5000;

SELECT sku,precio_unitario FROM productos WHERE precio_unitario >5000 AND precio_unitario <6000;

SELECT sku,precio_unitario FROM productos WHERE precio_unitario = 1500;

SELECT sku,existencia FROM productos WHERE existencia < 5;

truncate TABLE usuarios ; 

SELECT nombre_usuario,rol FROM usuarios WHERE rol = 1;

