SELECT clientes.nombres, clientes.primer_apellido, clientes.segundo_apellido, clientes.fecha, clientes.telefono, clientes.email, direcciones.calle, direcciones.colonia, direcciones.cp,
direcciones.municipio, direcciones.estado, direcciones.pais
FROM clientes,direcciones WHERE clientes.id_direccion = direcciones.id_direccion;
